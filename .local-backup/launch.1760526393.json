{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug: Uvicorn (.env.dev)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--host",
        "127.0.0.1",
        "--port",
        "25119",
        "--reload",
        "--env-file",
        "${workspaceFolder}/.env.dev"
      ],
      "envFile": "${workspaceFolder}/.env.dev",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "jinja": true,
      "console": "integratedTerminal"
    }
  ]
}
